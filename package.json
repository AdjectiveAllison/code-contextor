{
  "name": "code-contextor",
  "version": "0.1.0",
  "description": "A tool to extract context from code repositories for use with language models",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "bun run app.js",
    "dev": "bun --watch run app.js"
  },
  "keywords": [
    "code",
    "context",
    "nlp",
    "language-model"
  ],
  "author": "Allison Durham",
  "license": "MIT",
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "clipboardy": "^4.0.0",
    "commander": "^12.1.0",
    "ignore": "^5.3.2",
    "onnxruntime-node": "^1.18.0"
  },
  "bin": {
    "code-contextor": "./app.js"
  },
  "files": [
    "app.js",
    "fileFilter.js",
    "fileProcessor.js",
    "tokenizer.js",
    "outputFormatter.js"
  ]
}
